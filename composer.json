{
    "name": "phower/arrays",
    "description": "Array based classes to handle collections, stacks and queues in PHP.",
    "homepage": "https://github.com/phower/arrays",
    "issues": "https://github.com/phower/arrays/issues",
    "type": "library",
    "keywords": [
        "php",
        "array",
        "collection",
        "stack",
        "queue"
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Arrays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Arrays\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Ferreira",
            "email": "pedro@phower.com"
        }
    ]
}
